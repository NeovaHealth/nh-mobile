<?xml version='1.0' encoding='utf-8'?>
<widget id="org.openeobs.nfc" version="0.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <feature name="CorHttpd">
        <param name="android-package" value="com.rjfun.cordova.httpd.CorHttpd" />
    </feature>
    <feature name="NfcPlugin">
        <param name="android-package" value="com.chariotsolutions.nfc.plugin.NfcPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>openeObs</name>
    <description>
        open-eObs for the MioCare range of devices
    </description>
    <author email="office@neovahealth.co.uk" href="http://www.neovahealth.co.uk">
        Neova Health open-eObs Team
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="*" />
</widget>
